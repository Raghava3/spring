<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
<bean id="triangle" class="com.bridgelabz.secondautowired.model.Triangle" autowire="byName">
</bean>

<bean id="pointA" class="com.bridgelabz.secondautowired.model.Point">
<property name="x" value="10"></property>
<property name="y" value="10"></property>
</bean>

<bean id="pointB" class="com.bridgelabz.secondautowired.model.Point">
<property name="x" value="20"></property>
<property name="y" value="20"></property>
</bean>

<bean id="pointC" class="com.bridgelabz.secondautowired.model.Point">
<property name="x" value="30"></property>
<property name="y" value="30"></property>
</bean>
</beans>